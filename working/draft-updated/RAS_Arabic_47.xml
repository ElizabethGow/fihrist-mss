                                <title xml:lang="ar-Latn-x-lc">Faṣl fī jumal min al-ẓulm</title>
                                <title xml:lang="ar-Latn-x-lc" type="alt"/>
                                <title xml:lang="fa-Latn-x-lc" type="alt"/>
                                <title xml:lang="ar">فصل في جمل من الظلم</title>
                                <title xml:lang="ar" type="alt"/>
