                                &#x648;&#x627;&#x644;&#x648;&#x642;&#x62A; &#x636;&#x64A;&#x642; &#x648;&#x627;&#x644;&#x642;&#x636;&#x627; &#x639;&#x633;&#x631; &#x648;&#x627;&#x644;&#x62A;&#x62C;&#x631;&#x628;&#x629; &#x62E;&#x637;&#x631; &#x648;&#x642;&#x62F; &#x64A;&#x646;&#x628;&#x63A;&#x649; &#x644;&#x643; &#x627;&#x646; &#x644;&#x627; &#x64A;&#x642;&#x62A;&#x635;&#x631; &#x639;&#x644;&#x649; &#x62A;&#x648;&#x62E;&#x64A;
                                &#x641;&#x639;&#x644; &#x645;&#x627; &#x64A;&#x646;&#x628;&#x63A;&#x649; &#x62F;&#x648;&#x646; &#x627;&#x646; &#x64A;&#x643;&#x648;&#x646; &#x645;&#x627; &#x64A;&#x641;&#x639;&#x644;&#x647; &#x627;&#x644;&#x645;&#x631;&#x64A;&#x636; &#x648;&#x645;&#x646; &#x64A;&#x62D;&#x636;&#x631;&#x647; &#x643;&#x630;&#x644;&#x643; &#x648;&#x627;&#x644;&#x627;&#x634;&#x64A;&#x622; &#x627;&#x644;&#x62A;&#x649;
                                &#x645;&#x646; &#x62E;&#x627;&#x631;&#x62C; &#x628; &#x627;&#x646; &#x643;&#x627;&#x646; &#x645;&#x627; &#x64A;&#x633;&#x62A;&#x641;&#x631;&#x63A; &#x645;&#x646; &#x627;&#x644;&#x628;&#x62F;&#x646; &#x639;&#x646;&#x62F; &#x627;&#x633;&#x62A;&#x637;&#x644;&#x627;&#x642; &#x627;&#x644;&#x628;&#x637;&#x646; </incipit>
              <explicit>&#x648;&#x64A;&#x646;&#x628;&#x63A;&#x649; &#x627;&#x64A;&#x636;&#x627; &#x627;&#x646; &#x62A;&#x632;&#x646; &#x642;&#x648;&#x647; &#x627;&#x644;&#x645;&#x631;&#x64A;&#x636; &#x641;&#x62A;&#x639;&#x644;&#x645; &#x647;&#x644; &#x62A;&#x62B;&#x628;&#x62A; &#x627;&#x644;&#x649; &#x648;&#x642;&#x62A; &#x645;&#x646;&#x62A;&#x647;&#x649;
