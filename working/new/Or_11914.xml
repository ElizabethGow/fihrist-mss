            <msItem>
              <author>
                <persName xml:lang="per-Latn-x-lc" type="standard"/>
                <persName xml:lang="ara-Latn-x-lc" type="org"/>
                <persName xml:lang="eng" type="desc">Various calligraphers</persName>
                <persName xml:lang="per">
                  <surname/>
                  <forename/>
                  <addName xml:lang="per" type="laqab"/>
                  <addName xml:lang="ara" type="kunyah"/>
                  <addName xml:lang="ara" type="khitab"/>
