                <persName xml:lang="per-Latn-x-lc" type="standard">Muʻīn al-Dīn Farāhī, d. 1501 or 2</persName>
                <persName xml:lang="per">معين الدين فراهى</persName>
                <persName xml:lang="per-Latn-x-lc" type="alt">Muʻīn Miskīn</persName>
