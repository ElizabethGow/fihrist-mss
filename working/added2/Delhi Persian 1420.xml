            <msItem n="6" xml:id="a6" class="http://lccn.loc.gov/sh85072967 http://lccn.loc.gov/n2007204421">
              <locus from="272v" to="364r">ff. 272v-364r</locus>
              <author>
                <persName xml:lang="per-Latn-x-lc" type="supplied">Mullā Shāh Lisān
                                    Allāh (992-1072/1584-1661)</persName>
                <persName xml:lang="per-Latn-x-lc" type="desc">Qādirī saint and
                                    spiritual director of Dārā Shikūh</persName>
                <persName xml:lang="per" type="alt"/>
                <persName xml:lang="per">ملا شاه لسان الله<surname/>
                  <forename/>
                  <addName xml:lang="ara" type="laqab"/>
                  <addName xml:lang="ara" type="kunyah"/>
                  <addName xml:lang="ara" type="khitab"/>
                  <addName xml:lang="ara" type="nisbah"/>
                </persName>
              </author>
              <title xml:lang="per-Latn-x-lc">Tafsīr-i Shāh va Shāh-'i tafāsīr</title>
              <title xml:lang="per">تفسیر شاه و شاۀ تفاسیر</title>
              <title xml:lang="per-Latn-x-lc" type="alt"/>
              <title xml:lang="per" type="alt"/>
              <title xml:lang="eng" type="desc"/>
              <incipit xml:lang="per"/>
              <explicit xml:lang="per"/>
              <note>An unfinished commentary on the Qurʼān, partly in Persian and
                                partly in Arabic</note>
              <note>The text of the Qurʼān is given in fragments, followed, as far as
                                Sūrah 2:198, by a Persian paraphrase or translation. After verse 198
                                the Persian translation ceases and the comments are in Arabic and
                                usually very brief</note>
              <listBibl>
                <bibl>For a more detailed description see Persian III,  .</bibl>
                <bibl/>
              </listBibl>
              <textLang mainLang="per">Persian</textLang>
            </msItem>
            <msItem n="7" xml:id="a7" class="http://lccn.loc.gov/sh85072967 http://lccn.loc.gov/n2007204421">
              <locus from="365v" to="412r">ff. 365v-412r</locus>
              <author>
                <persName xml:lang="per-Latn-x-lc" type="supplied">Mullā Shāh Lisān
                                    Allāh (992-1072/1584-1661)</persName>
                <persName xml:lang="per-Latn-x-lc" type="desc">Qādirī saint and
                                    spiritual director of Dārā Shikūh</persName>
                <persName xml:lang="per" type="alt"/>
                <persName xml:lang="per">ملا شاه لسان الله<surname/>
                  <forename/>
                  <addName xml:lang="ara" type="laqab"/>
                  <addName xml:lang="ara" type="kunyah"/>
                  <addName xml:lang="ara" type="khitab"/>
                  <addName xml:lang="ara" type="nisbah"/>
                </persName>
              </author>
              <title xml:lang="per-Latn-x-lc">Tafsīr-i Sūrah-i Yūsuf</title>
              <title xml:lang="per"/>
              <title xml:lang="per-Latn-x-lc" type="alt"/>
              <title xml:lang="per" type="alt"/>
              <title xml:lang="eng" type="desc"/>
              <incipit xml:lang="per"/>
              <explicit xml:lang="ara" defective="true"/>
              <note>A commentary without title on Sūrah 12 of the Qurʼān</note>
              <note>As in the previous work, the Qurʼānic text is given in fragments,
                                usually followed by a short metrical paraphrase in Persian</note>
              <note>Composed in<date calendar="Hijri-qamari" when="1647">1057</date>
              </note>
              <listBibl>
                <bibl>For a more detailed description see Persian III,  .</bibl>
              </listBibl>
              <textLang mainLang="per">Persian</textLang>
            </msItem>
